{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": [
    "dependencies",
    "no changelog"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^axum"
      ],
      "groupName": "axum crates"
    },
    {
      "matchPackagePatterns": [
        "^poem"
      ],
      "groupName": "poem crates"
    },
    {
      "matchPackagePatterns": [
        "^tower"
      ],
      "groupName": "tower crates"
    },
    {
      "matchPackagePatterns": [
        "^serde"
      ],
      "groupName": "serde crates"
    }
  ]
}